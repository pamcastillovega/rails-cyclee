<%= simple_form_for :lane, url: lanes_path, method: :get do |f| %>
  <%= f.input :address,
    input_html: {data: {address_autocomplete_target: "address"}, class: "d-none"},
    wrapper_html: {data: {controller: "address-autocomplete", address_autocomplete_api_key_value: ENV["MAPBOX_API_KEY"]}, placeholder: "Location"} %>
  <%= f.button :submit, value: "Search", class: "btn btn-banner" %>
  <% end %>
